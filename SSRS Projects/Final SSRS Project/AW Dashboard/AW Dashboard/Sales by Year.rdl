    <DataSource Name="AdventureWorks2014">
      <DataSourceReference>AdventureWorks2014</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>61b18700-187f-43fa-a06b-adbf31bd3234</rd:DataSourceID>
    </DataSource>
