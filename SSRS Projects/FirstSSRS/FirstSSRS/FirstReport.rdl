  <DataSets>
    <DataSet Name="Territory">
      <Query>
        <DataSourceName>AdventureWorks2014</DataSourceName>
        <CommandText>SELECT sh.OrderDate,TotalDue,st.Name AS TerritoryName,

st.[Group] AS TerritoryGroup

FROM [AdventureWorks2014].[Sales].[SalesOrderHeader] sh
LEFT OUTER JOIN [AdventureWorks2014].[Sales].[SalesTerritory] st
ON sh.TerritoryID = st.TerritoryID</CommandText>
      </Query>
